{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "interface-name": [true, "never-prefix"],
    "member-access": false,
    "no-debugger": false,
    "no-empty": false,
    "jsx-no-lambda": false,
    "no-namespace": false,
    "jsx-boolean-value": false,
    "no-console": false
  }
}
