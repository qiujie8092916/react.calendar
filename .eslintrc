{
  "extends": ["standard", "standard-react"],
  "globals": {
    "$": true,
    "process": true,
    "__dirname": true
  },
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "quotes": 0,
    "jsx-quotes": 0,
    "semi": ["error", "always"],
    "space-before-function-paren": 0,
    "camelcase": 0,
    "handle-callback-err": 1,
    "no-debugger": 0,
    "react/prop-types": 0,
    "standard/computed-property-even-spacing": 0
  }
}
